<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	
	<navigation-rule>
		<from-view-id>/pages/init.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>360_VIEW</from-outcome>
			<to-view-id>/pages/vista360.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>ACCOUNTS</from-outcome>
			<to-view-id>/pages/bank_account.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>PSE</from-outcome>
			<to-view-id>/pages/pse.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>	
		<navigation-case>
			<from-outcome>CREDIT_CARD</from-outcome>
			<to-view-id>/pages/credit_card.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/vista360.xhtml</from-view-id>		
		<navigation-case>
			<from-outcome>INIT</from-outcome>
			<to-view-id>/pages/init.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>		
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/bank_account.xhtml</from-view-id>		
		<navigation-case>
			<from-outcome>INIT</from-outcome>
			<to-view-id>/pages/init.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>		
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/pse.xhtml</from-view-id>		
		<navigation-case>
			<from-outcome>INIT</from-outcome>
			<to-view-id>/pages/init.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>		
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/credit_card.xhtml</from-view-id>		
		<navigation-case>
			<from-outcome>INIT</from-outcome>
			<to-view-id>/pages/init.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>		
	</navigation-rule>
	
</faces-config>
